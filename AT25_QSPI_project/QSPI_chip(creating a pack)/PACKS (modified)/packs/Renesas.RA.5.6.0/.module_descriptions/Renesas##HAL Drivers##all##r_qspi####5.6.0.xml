        <declarations>
qspi_instance_ctrl_t ${module.driver.qspi.name}_ctrl;

static const spi_flash_erase_command_t ${module.driver.qspi.name}_erase_command_list[] =
{
#if ${module.driver.qspi.sector_erase_size} > 0
    {.command = ${module.driver.qspi.sector_erase_command},     .size = ${module.driver.qspi.sector_erase_size} },
#endif
#if ${module.driver.qspi.block_erase_32kb_size} > 0
    {.command = ${module.driver.qspi.block_erase_32kb_command}, .size = ${module.driver.qspi.block_erase_32kb_size} },
#endif
#if ${module.driver.qspi.block_erase_size} > 0
    {.command = ${module.driver.qspi.block_erase_command},      .size = ${module.driver.qspi.block_erase_size} },
#endif
#if ${module.driver.qspi.chip_erase_command} > 0
    {.command = ${module.driver.qspi.chip_erase_command},       .size  = SPI_FLASH_ERASE_SIZE_CHIP_ERASE         },
#endif
};
